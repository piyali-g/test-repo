                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Technology Name</value>
                    </name>
                    <type>Hidden</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>technologyName</key>
                    <multiple>false</multiple>
                    <value>mobile-web</value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
</mojos>
